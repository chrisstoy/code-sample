{
  "name": "webapp",
  "version": "1.0.0",
  "description": "Angular WebApp for CodeSample app",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --code-coverage",
    "test-debug": "ng test",
    "build-docker": "npm run build && docker build -t codesample/webapp .",
    "run-docker": "docker run --name codesample_webapp -d -p 80:80 codesample/webapp",
    "kill-docker": "docker kill codesample_webapp && docker rm codesample_webapp",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.12",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.12",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.12",
    "@angular/cli": "~12.2.12",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/elements": "^13.0.1",
    "@babel/core": "^7.16.0",
    "@compodoc/compodoc": "^1.1.15",
    "@storybook/addon-actions": "^6.4.0-rc.2",
    "@storybook/addon-essentials": "^6.4.0-rc.2",
    "@storybook/addon-links": "^6.4.0-rc.2",
    "@storybook/angular": "^6.4.0-rc.2",
    "@storybook/builder-webpack5": "^6.4.0-rc.2",
    "@storybook/manager-webpack5": "^6.4.0-rc.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@webcomponents/custom-elements": "^1.5.0",
    "babel-loader": "^8.2.3",
    "jasmine-core": "~3.8.0",
    "jasmine-marbles": "^0.8.4",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-mocks": "^12.5.0",
    "typescript": "~4.3.5"
  }
}
